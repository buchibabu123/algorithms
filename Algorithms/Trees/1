/*
 *
 * BST Operations
 * 1.) insert
 * 2.) search
 * 3.) inorder,preorder,postorder
 * 4.) delete node
 *
 * */

#include <stdio.h>
#include <iostream>

using namespace std;

typedef struct Node
{

	int data;
	struct Node *left;
	struct Node *right;

} Node;

Node *NewNode(Node *root , int key)
{

    root = (Node*)malloc(sizeof(Node));
    root->left = root->right = NULL;
    root->data = key;
    return root;

}

void insertNode(Node *root,int key)
{

    if(root == NULL)
        return NewNode(key);

}

struct Node *head = NULL;

int  main()
{


	return 0;
}

